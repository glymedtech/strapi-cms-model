{
  "kind": "collectionType",
  "collectionName": "days_of_glymeds",
  "info": {
    "singularName": "days-of-glymed",
    "pluralName": "days-of-glymeds",
    "displayName": "Days of Glymed"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "banner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::banner.banner"
    },
    "role": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "web",
        "pro",
        "retial"
      ]
    },
    "startDate": {
      "type": "datetime",
      "required": true
    },
    "endDate": {
      "type": "datetime",
      "required": true
    },
    "countdownSectionTitle": {
      "type": "component",
      "component": "shared.text-with-color",
      "repeatable": false,
      "required": true
    },
    "countdownSectionDealsEndingTitle": {
      "type": "component",
      "component": "shared.text-with-color",
      "repeatable": false,
      "required": true
    },
    "countdownSectionDealsEndedTitle": {
      "type": "component",
      "component": "shared.text-with-color",
      "repeatable": false
    },
    "countdownTimerInstruction": {
      "type": "component",
      "component": "shared.text-with-color",
      "repeatable": false
    },
    "featured_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::featured-product.featured-product"
    }
  }
}
