{
  "kind": "collectionType",
  "collectionName": "feature_pages",
  "info": {
    "singularName": "feature-page",
    "pluralName": "feature-pages",
    "displayName": "FeaturePage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "component",
      "component": "shared.text-with-color",
      "repeatable": false
    },
    "subTitle": {
      "type": "component",
      "component": "shared.text-with-color",
      "repeatable": false
    },
    "description": {
      "type": "component",
      "component": "shared.text-with-color",
      "repeatable": false
    },
    "asset": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files",
        "videos"
      ]
    },
    "videoPoster": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "portalId": {
      "type": "string"
    },
    "formId": {
      "type": "string"
    },
    "featuredProductsTitle": {
      "type": "component",
      "component": "shared.text-with-color",
      "repeatable": false
    },
    "featuredProductsSkuList": {
      "type": "component",
      "component": "shared.list",
      "repeatable": true
    },
    "startTime": {
      "type": "datetime",
      "required": true
    },
    "endTime": {
      "type": "datetime",
      "required": false
    },
    "banner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::banner.banner"
    },
    "role": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "web",
        "retail",
        "pro",
        "school"
      ],
      "required": true,
      "min": 1,
      "max": 4
    },
    "videoTitle": {
      "type": "component",
      "component": "shared.text-with-color",
      "repeatable": false
    },
    "formTitle": {
      "type": "component",
      "component": "shared.text-with-color",
      "repeatable": false
    }
  }
}
