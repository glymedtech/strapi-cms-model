{
  "kind": "singleType",
  "collectionName": "protocol_pages",
  "info": {
    "singularName": "protocol-page",
    "pluralName": "protocol-pages",
    "displayName": "ProtocolPage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "banner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::banner.banner"
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "unique": true
    },
    "guestUserMessageTitle": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.text-with-color",
      "repeatable": false
    },
    "guestUserMessageSubtitle": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.text-with-color",
      "repeatable": false
    }
  }
}
